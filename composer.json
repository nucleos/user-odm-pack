{
    "name": "nucleos/user-odm-pack",
    "description": "A pack for NucleosUserBundle with MongoDB support",
    "license": "MIT",
    "type": "symfony-pack",
    "authors": [
        {
            "name": "Christian Gripp",
            "email": "mail@core23.de"
        }
    ],
    "homepage": "https://nucleos.rocks",
    "require": {
        "php": "^8.0",
        "doctrine/mongodb-odm": "^1.1 || ^2.0",
        "doctrine/mongodb-odm-bundle": "^4.1",
        "nucleos/user-bundle": "^2.0"
    },
    "require-dev": {
        "alcaeus/mongo-php-adapter": "^1.1",
        "ergebnis/composer-normalize": "^2.0.1"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "platform": {
            "ext-mongo": "1.6.16"
        }
    }
}
